  number = {4},
  journal = {European Journal of Applied Physiology},
  author = {Eisenmann, Joey C and Wickel, Eric E and Kelly, Scott A and Middleton, Kevin M and Garland Jr, Theodore},
  year = {2009},
  pages = {613--619},
  file = {2009 Eisenmann et al Day-to-day variability in voluntary wheel running among genetically.pdf:/Users/kmm/Dropbox/PDF_Zotero/2009 Eisenmann et al Day-to-day variability in voluntary wheel running among genetically.pdf:application/pdf}
}

